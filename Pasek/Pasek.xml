<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs title="Call Recording"
        description="Call Recording">
        <Require feature="settitle" />
        <Require feature="dynamic-height" />
        <Require feature="pubsub-2" />
        <Require feature="setprefs" />
        <Require feature="loadingindicator">
            <Param name="manual-dismiss">false</Param>
            <Param name="loading-timeout">10</Param>
        </Require>
    </ModulePrefs>

    <Content type="html">
        <![CDATA[
            <!DOCTYPE html>
            <!--[if lt IE 7]>      <html class="lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
            <!--[if IE 7]>         <html class="lt-ie9 lt-ie8"> <![endif]-->
            <!--[if IE 8]>         <html class="lt-ie9"> <![endif]-->
            <!--[if gt IE 8]>      <html> <!--<![endif]-->
            <head>
                <!-- Styling -->
                <link rel="stylesheet" href="bootstrap/2.3.2/css/bootstrap.min.css" type="text/css" />
                <link rel="stylesheet" href="CallRecording.css" type="text/css" />

           
                <!-- Bootstrap -->
                <script type="text/javascript" src="bootstrap/2.3.2/js/bootstrap.min.js"></script>
               
                <!-- Finesse Library -->
                <script type="text/javascript" src="finesse-10.0.1.js"></script>
            
            </head>

            <body class="claro">
                <div id="successMsg" class="alert alert-success" style="display: none">
                    <strong>Chat - pasek statusu chata (zmiana statusu informajce o wiadomosci)</strong>
                </div>
            </body>


        ]]>
    </Content>
</Module>